{
    "name": "boxblinkracer/phpunuhi",
    "version": "0.6.1",
    "license": "MIT",
    "description": "Easy tool to work with translation files for validation, exports and imports.",
    "keywords": [
    ],
    "authors": [
        {
            "name": "Christian Dangl"
        }
    ],
    "bin": [
        "bin/phpunuhi"
    ],
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "deeplcom/deepl-php": "^1.2",
        "google/cloud-translate": "^1.12",
        "symfony/console": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "phpstan/phpstan": "*",
        "friendsofphp/php-cs-fixer": "^2.18"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    }
}
