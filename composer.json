{
  "name": "boxblinkracer/phpunuhi",
  "version": "0.6.0",
  "license": "MIT",
  "description": "Easy tool to work with translation files for validation, imports and exports.",
  "keywords": [
  ],
  "authors": [
    {
      "name": "Christian Dangl"
    }
  ],
  "bin": [
    "bin/phpunuhi"
  ],
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "require": {
    "php": ">=7.2",
    "ext-dom": "*",
    "ext-simplexml": "*",
    "symfony/console": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "phpstan/phpstan": "*",
    "friendsofphp/php-cs-fixer": "^2.18"
  },
  "autoload": {
    "classmap": [
      "src/"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  }
}
